# Database Configuration - Replace with your Neon PostgreSQL URL
DATABASE_URL="postgresql://username:password@hostname.neon.tech:5432/database_name?sslmode=require"

# Gemini API Configuration - Get from Google AI Studio
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# File Storage Directories
OUTPUT_DIR=./output

# Blender Configuration (should work with your Homebrew installation)
BLENDER_EXECUTABLE=blender
BLENDER_TIMEOUT=30000

# CORS Configuration
FRONTEND_URL=http://localhost:3000
CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...
